$primary = #ff8800
$secondary = #202225
$dark = #0a003b
$black = #12002C
$grayDark = #C1C1C1
$accent = #30E6FC
$grayLigth = #FFFFFF
$bg-body   = rgba(0,0,0,.25)
$nostr   = #ff8800
$grayDark2 = #727373
$grayDark3 = #656666

.text-primary
  color: $primary !important
.text-grey-dark
  color: $grayDark !important

.bg-nostr
  background-color: $nostr
.bg-grey-dark
  background-color: $grayDark
.bg-grey-dark-2
  background-color: $grayDark2
.bg-grey-dark-3
  background-color: $grayDark3
:root
  --q-primary #ff8800 !important
  --q-secondary #202225 !important
  --q-accent #30E6FC !important
  --q-dark #1D1D1D !important
  --q-light #76f903 !important
